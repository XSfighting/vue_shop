{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport cityData from \"@/components/order/citydata\";\nexport default {\n  name: \"Order\",\n\n  data() {\n    return {\n      queryInfo: {\n        query: '',\n        pagenum: 1,\n        pagesize: 10\n      },\n      orderList: [],\n      total: 0,\n      AddressVisible: false,\n      addressForm: {\n        address1: '',\n        address2: ''\n      },\n      addressFormRule: {\n        address1: [{\n          required: true,\n          message: '请选择省市区县地址',\n          trigger: 'blur'\n        }],\n        address2: [{\n          required: true,\n          message: '请填写详细地址',\n          trigger: 'blur'\n        }]\n      },\n      cityData,\n      progressVisible: false,\n      progressInfo: []\n    };\n  },\n\n  created() {\n    this.getOrderList();\n  },\n\n  methods: {\n    async getOrderList() {\n      const {\n        data: res\n      } = await this.$http.get('orders', {\n        params: this.queryInfo\n      });\n      if (res.meta.status !== 200) return this.$message.error('获取订单列表失败！'); // console.log(res);\n\n      this.orderList = res.data.goods;\n      this.total = res.data.total;\n      console.log(this.orderList);\n    },\n\n    handleSizeChange(newSize) {\n      this.queryInfo.pagesize = newSize;\n      this.getOrderList();\n    },\n\n    handleCurrentChange(newPage) {\n      this.queryInfo.pagenum = this.getOrderList();\n    },\n\n    showBox() {\n      this.AddressVisible = true;\n    },\n\n    addAddressDialogClosed() {\n      this.$refs.addressFormRef.resetFields();\n    },\n\n    async showProgressBox() {\n      const {\n        data: res\n      } = await this.$http.get('/kuaidi/1106975712662');\n      this.progressVisible = true;\n      if (res.meta.status !== 200) return this.$message.error('获取物流信息失败！'); // console.log(res.data);\n\n      this.progressInfo = res.data;\n      console.log(this.progressInfo);\n    }\n\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyFA;AACA;EACAA,aADA;;EAEAC;IACA;MACAC;QACAC,SADA;QAEAC,UAFA;QAGAC;MAHA,CADA;MAMAC,aANA;MAOAC,QAPA;MAQAC,qBARA;MASAC;QACAC,YADA;QAEAC;MAFA,CATA;MAaAC;QACAF,WACA;UAAAG;UAAAC;UAAAC;QAAA,CADA,CADA;QAIAJ,WACA;UAAAE;UAAAC;UAAAC;QAAA,CADA;MAJA,CAbA;MAqBAC,QArBA;MAsBAC,sBAtBA;MAuBAC;IAvBA;EAyBA,CA5BA;;EA6BAC;IACA;EACA,CA/BA;;EAgCAC;IACA;MACA;QAAAnB;MAAA;QACAoB;MADA;MAEA,qEAHA,CAIA;;MACA;MACA;MACAC;IACA,CATA;;IAUAC;MACA;MACA;IACA,CAbA;;IAcAC;MACA,yBACA,mBADA;IAEA,CAjBA;;IAkBAC;MACA;IACA,CApBA;;IAqBAC;MACA;IACA,CAvBA;;IAwBA;MACA;QAAAzB;MAAA;MACA;MACA,qEAHA,CAIA;;MACA;MACAqB;IACA;;EA/BA;AAhCA","names":["name","data","queryInfo","query","pagenum","pagesize","orderList","total","AddressVisible","addressForm","address1","address2","addressFormRule","required","message","trigger","cityData","progressVisible","progressInfo","created","methods","params","console","handleSizeChange","handleCurrentChange","showBox","addAddressDialogClosed"],"sourceRoot":"src/components/order","sources":["Order.vue"],"sourcesContent":["<template>\r\n<div>\r\n  <!-- 面包屑导航区域 -->\r\n  <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n    <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n    <el-breadcrumb-item>订单管理</el-breadcrumb-item>\r\n    <el-breadcrumb-item>订单列表</el-breadcrumb-item>\r\n  </el-breadcrumb>\r\n\r\n <el-card>\r\n  <!--  搜索与添加区-->\r\n  <el-row>\r\n    <el-col :span=\"8\">\r\n      <el-input placeholder=\"请输入内容\" >\r\n        <el-button slot=\"append\" icon=\"el-icon-search\"></el-button>\r\n      </el-input>\r\n    </el-col>\r\n  </el-row>\r\n\r\n   <el-table :data=\"orderList\" stripe border >\r\n     <el-table-column type=\"index\"></el-table-column>\r\n     <el-table-column label=\"订单编号\" prop=\"order_number\"></el-table-column>\r\n     <el-table-column label=\"订单价格\" prop=\"order_price\"></el-table-column>\r\n     <el-table-column label=\"是否支付\" prop=\"pay_status\">\r\n       <template v-slot=\"scope\">\r\n         <el-tag type=\"success\" v-if=\"scope.row.pay_status == '1'\">已付款</el-tag>\r\n         <el-tag type=\"danger\" v-else>未付款</el-tag>\r\n       </template>\r\n     </el-table-column>\r\n     <el-table-column label=\"是否发货\" prop=\"is_send\"></el-table-column>\r\n     <el-table-column label=\"下单时间\" prop=\"create_time\">\r\n       <template v-slot=\"scope\">\r\n         {{scope.row.create_time | dateFormat}}\r\n       </template>\r\n     </el-table-column>\r\n     <el-table-column label=\"操作\">\r\n       <template v-slot=\"scope\">\r\n         <el-tooltip  effect=\"dark\" content=\"修改地址\" placement=\"top\" :enterable=\"false\">\r\n         <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\" @click=\"showBox\"></el-button>\r\n         </el-tooltip>\r\n\r\n         <el-tooltip  effect=\"dark\" content=\"查询物流信息\" placement=\"top\" :enterable=\"false\">\r\n         <el-button type=\"success\" icon=\"el-icon-location\" size=\"mini\" @click=\"showProgressBox\"></el-button>\r\n         </el-tooltip>\r\n       </template>\r\n     </el-table-column>\r\n\r\n   </el-table>\r\n\r\n   <el-pagination\r\n       @size-change=\"handleSizeChange\"\r\n       @current-change=\"handleCurrentChange\"\r\n       :current-page=\"queryInfo.pagenum\"\r\n       :page-sizes=\"[10, 20, 30, 100]\"\r\n       :page-size=\"queryInfo.pagesize\"\r\n       layout=\"total, sizes, prev, pager, next, jumper\"\r\n       :total=\"total\">\r\n   </el-pagination>\r\n</el-card>\r\n\r\n  <el-dialog\r\n      title=\"修改地址\" :visible.sync=\"AddressVisible\" width=\"50%\" @close=\"addAddressDialogClosed\">\r\n    <el-form :model=\"addressForm\" :rules=\"addressFormRule\" ref=\"addressFormRef\" label-width=\"100px\">\r\n      <el-form-item  label=\"省市区/县\" prop=\"address1\">\r\n   <el-cascader :options=\"cityData\" v-model=\"addressForm.address1\"></el-cascader>\r\n      </el-form-item>\r\n      <el-form-item  label=\"详细地址\" prop=\"address1\">\r\n        <el-input v-model=\"addressForm.address2\"></el-input>\r\n      </el-form-item>\r\n    </el-form>\r\n\r\n    <span slot=\"footer\" class=\"dialog-footer\">\r\n    <el-button @click=\"AddressVisible = false\">取 消</el-button>\r\n    <el-button type=\"primary\" @click=\"AddressVisible = false\">确 定</el-button>\r\n  </span>\r\n  </el-dialog>\r\n\r\n  <el-dialog title=\"物流进度\" :visible.sync=\"progressVisible\" width=\"50%\" >\r\n    <el-timeline>\r\n      <el-timeline-item v-for=\"(item,index) in progressInfo\"\r\n          :key=\"index\" :timestamp=\"item.time\" color=\"green\">\r\n        {{item.context}}\r\n      </el-timeline-item>\r\n    </el-timeline>\r\n  </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport cityData from \"@/components/order/citydata\";\r\nexport default {\r\n  name: \"Order\",\r\n  data() {\r\n    return {\r\n      queryInfo: {\r\n        query: '',\r\n        pagenum: 1,\r\n        pagesize: 10\r\n      },\r\n      orderList: [],\r\n      total: 0,\r\n      AddressVisible: false,\r\n      addressForm: {\r\n        address1: '',\r\n        address2: ''\r\n      },\r\n      addressFormRule: {\r\n        address1: [\r\n          { required: true, message: '请选择省市区县地址' , trigger: 'blur'}\r\n        ],\r\n        address2: [\r\n          { required: true, message: '请填写详细地址' , trigger: 'blur'}\r\n        ]\r\n      },\r\n      cityData,\r\n      progressVisible: false,\r\n      progressInfo: []\r\n    }\r\n  },\r\n  created() {\r\n    this.getOrderList()\r\n  },\r\n  methods: {\r\n    async getOrderList() {\r\n     const {data: res} = await this.$http.get('orders',{\r\n       params: this.queryInfo})\r\n      if (res.meta.status !== 200) return this.$message.error('获取订单列表失败！')\r\n      // console.log(res);\r\n      this.orderList = res.data.goods\r\n      this.total = res.data.total\r\n      console.log(this.orderList);\r\n    },\r\n    handleSizeChange(newSize) {\r\n      this.queryInfo.pagesize = newSize\r\n      this.getOrderList()\r\n    },\r\n    handleCurrentChange(newPage) {\r\n      this.queryInfo.pagenum =\r\n          this.getOrderList()\r\n    },\r\n    showBox() {\r\n      this.AddressVisible = true\r\n    },\r\n    addAddressDialogClosed() {\r\n      this.$refs.addressFormRef.resetFields()\r\n    },\r\n    async showProgressBox() {\r\n     const {data: res} = await this.$http.get('/kuaidi/1106975712662')\r\n      this.progressVisible = true\r\n      if (res.meta.status !== 200) return this.$message.error('获取物流信息失败！')\r\n      // console.log(res.data);\r\n      this.progressInfo = res.data\r\n      console.log(this.progressInfo);\r\n    },\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.el-cascader {\r\n  width: 100%;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}